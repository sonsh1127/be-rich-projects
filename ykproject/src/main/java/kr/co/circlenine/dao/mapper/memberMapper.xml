<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.circlenine.dao.MemberDao">
	<select id="selectOneById" parameterType="String" resultType="map">
		select * from member where member_id = #{id}
	</select>
	<select id="selectOneByNum" parameterType="int" resultType="map">
		select * from member where member_num = #{num}
	</select>
	<select id = "selectMemberAuthorities" parameterType="string" resultType="string">
		select authority from authorities where username = #{memberId}
	</select>
</mapper>
